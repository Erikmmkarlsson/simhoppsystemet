@model IEnumerable<simhoppsystemet.Models.CompetitionCompetitor>

@{
    ViewData["Title"] = "Divers";
}

<h1>Divers</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Competition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Competitor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Competition.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Competitor.Gender)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CompetitionCompetitorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CompetitionCompetitorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CompetitionCompetitorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
